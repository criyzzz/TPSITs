package video;

public class Video {
    public String name;
    public int duration;

    public Video(String name, int duration) {
        this.name = name;
        this.duration = duration;
    }

    public String getName() {
        return name;
    }

    public int getDuration() {
        return duration;
    }

    public void reduceDuration(int seconds) {
        duration -= seconds;
    }
}
